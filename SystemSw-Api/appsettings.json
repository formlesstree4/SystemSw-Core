{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Extron": {
    "Port": "/dev/ttyUSB0",
    "ReadTimeout": 1000,
    "WriteTimeout": 1000
  },
  "ChannelMappings": {
    "1": "Super Nintendo",
    "2": "Wii",
    "3": "Nintendo 64",
    "4": "PS2 (JP)",
    "5": "Famicom",
    "7": "Sega Memecast",
    "8": "Playstation 2 (EN)"
  },
  "Kestrel": {
    "EndPoints": {
        "Http": {
            "Url": "http://*:8000"
        }
    }
  },
  "AllowedHosts": "*"
}
